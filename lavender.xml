<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!--
    * target: lavender
    * rom: EvoX :3
    -->

    <!-- Remote -->
    <remote fetch="https://github.com/ParsimoniousLavender" name="org" />
	
    <!-- Device -->
    <project name="device_xiaomi_lavender" path="device/xiaomi/lavender" remote="org" revision="Evo-11-org" />
    <project name="device_xiaomi_sdm660-common" path="device/xiaomi/sdm660-common" remote="org" revision="Evo-11-org" />

    <!-- Kernel -->
    <project name="kernel_xiaomi_lavender" path="kernel/xiaomi/sdm660" remote="org" revision="eleven" />

    <!-- Vendor -->
    <project name="vendor_xiaomi_lavender" path="vendor/xiaomi/lavender" remote="org" revision="eleven" />
    <project name="vendor_xiaomi_sdm660-common" path="vendor/xiaomi/sdm660-common" remote="org" revision="Evo-11-org" />

    <!-- WLAN -->
    <remove-project name="android_hardware_qcom_wlan" />
    <project name="hardware_qcom-caf_wlan" path="hardware/qcom-caf/wlan" remote="org" revision="eleven" />

    <!-- Telephony -->
    <remove-project name="android_vendor_codeaurora_telephony" />
    <project name="android_vendor_codeaurora_telephony" path="vendor/codeaurora/telephony" remote="org" revision="elle" />

</manifest>

